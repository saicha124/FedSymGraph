modules = ["python-3.11"]
[agent]
expertMode = true
integrations = ["python_openai:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["glibcLocales"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "FedSymGraph Server"

[[workflows.workflow]]
name = "FedSymGraph Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python server.py --rounds 3 --min-clients 2"

[workflows.workflow.metadata]
outputType = "console"

[[ports]]
localPort = 8080
externalPort = 80
